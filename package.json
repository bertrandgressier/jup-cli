{
  "name": "jupiter-cli",
  "version": "1.0.0",
  "description": "A secure, multi-wallet CLI for trading on Solana via Jupiter DEX aggregator",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "jupiter": "dist/index.js"
  },
  "files": [
    "dist/**/*",
    "prisma/schema.prisma",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "validate": "npm run typecheck && npm run lint && npm run format:check",
    "prepublishOnly": "npm run validate && npm run build && npm test",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prepare": "husky",
    "postinstall": "prisma generate",
    "semantic-release": "semantic-release",
    "semantic-release:dry-run": "semantic-release --dry-run"
  },
  "keywords": [
    "jupiter",
    "solana",
    "dex",
    "trading",
    "cli",
    "wallet",
    "cryptocurrency",
    "defi",
    "blockchain"
  ],
  "author": {
    "name": "Bertrand Gressier",
    "email": "bertrand.gressier@gmail.com",
    "url": "https://github.com/bertrandgressier"
  },
  "license": "MIT",
  "homepage": "https://github.com/bertrandgressier/jupiter-cli#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bertrandgressier/jupiter-cli.git"
  },
  "bugs": {
    "url": "https://github.com/bertrandgressier/jupiter-cli/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@prisma/client": "^5.8.1",
    "@pythnetwork/client": "^2.21.0",
    "@solana/spl-token": "^0.3.11",
    "@solana/web3.js": "^1.89.1",
    "argon2": "^0.40.1",
    "axios": "^1.6.5",
    "big.js": "^6.2.1",
    "bs58": "^5.0.0",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "dotenv": "^16.3.2",
    "inquirer": "^9.2.12",
    "js-yaml": "^4.1.1",
    "ora": "^5.4.1",
    "pino": "^10.3.1",
    "pino-pretty": "^13.1.3",
    "pino-roll": "^4.0.0",
    "tweetnacl": "^1.0.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/big.js": "^6.2.2",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.11",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.2.7",
    "nock": "^13.5.0",
    "prettier": "^3.8.1",
    "prisma": "^5.8.1",
    "semantic-release": "^25.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,json,md}": [
      "prettier --write"
    ]
  }
}
